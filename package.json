{
  "name": "DinDin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:apk": "(cd android && ./gradlew assembleRelease)",
    "android:bundle": "(cd android && ./gradlew bundleRelease)",
    "check": "npm run flow | npm run lint",
    "detox:build": "detox build --configuration ios",
    "test:e2e": "detox build --configuration ios && detox test --configuration ios",
    "test:unit": "jest __tests__/unit/ --verbose",
    "flow": "flow",
    "ios": "react-native run-ios",
    "lint": "eslint --fix src/*.js src/**/*.js src/**/**/*.js src/**/**/**/*.js src/**/**/**/**/*.js __tests__/**/*.test.js",
    "optimization:png": "imageOptim ./src/assets/**/*.png",
    "optimization:svg": "npx svgo ./src/assets/**/*.svg --disable=removeViewBox",
    "prettier": "prettier src/*.js src/**/*.js src/**/**/*.js src/**/**/**/*.js src/**/**/**/**/*.js",
    "start": "react-native start",
    "visualizer": "npx visualize-bundle"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.0",
    "detox": "^17.6.1",
    "i18n-js": "^3.7.1",
    "immer": "^7.0.9",
    "mocha": "^8.1.3",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-navigation": "^6.11.0",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/runtime": "7.11.2",
    "@react-native-community/eslint-config": "2.0.0",
    "babel-jest": "26.3.0",
    "babel-plugin-root-import": "^6.5.0",
    "eslint": "7",
    "flow-bin": "^0.131.0",
    "husky": "^4.2.5",
    "jest": "26.4.0",
    "metro-react-native-babel-preset": "0.62.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "16.13.1",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
